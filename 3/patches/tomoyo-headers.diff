+struct group_member {
+	struct group_member *next;
+	const struct path_info *member_name;
+	u8 is_deleted;
+};
+
+struct group_entry {
+	struct group_entry *next;
+	const struct path_info *group_name;
+	struct group_member *first_member;
+};
